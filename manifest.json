{
  "name": "Iraninan-adblocker",
  "version": "1.0",
  "description": "Blocker to detect and remove ads",
  "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"],
  "background": {
	"scripts": ["blocked_domains.js", "background.js", "content_script.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "manifest_version": 2
}
